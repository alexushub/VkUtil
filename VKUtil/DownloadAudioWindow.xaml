<Window x:Class="VkUtil.DownloadAudioWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VkUtil"
        mc:Ignorable="d"
        Title="Download audio" Height="383.495" Width="423.3">
    <Grid x:Name="downLoadAudioWindow">
        <TextBox x:Name="searchTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="299"/>
        <Button x:Name="searchButton" Content="Search" HorizontalAlignment="Left" Margin="314,10,0,0" VerticalAlignment="Top" Width="91" RenderTransformOrigin="-0.109,0.967" Click="searchButton_Click"/>
        <ListBox x:Name="audioListBox" HorizontalAlignment="Left" Height="194" Margin="10,38,0,0" VerticalAlignment="Top" Width="395">
            <ListBox.ItemTemplate>
                <DataTemplate >
                    <WrapPanel>
                        <local:AudioInListControl></local:AudioInListControl>
                    </WrapPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>

    </Grid>
</Window>
